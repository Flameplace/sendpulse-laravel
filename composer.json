{
    "name": "flameplace/sendpulse-laravel",
    "description": "Laravel Package for Sendpulse API. Provides easy access to the sendpulse/sendpulse-rest-api-php library.",
    "keywords": [
        "flameplace",
        "sendpulse",
        "laravel"
    ],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Flameplace\\Sendpulse\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alexey",
            "email": "butochnikov@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/support": "5.7.* || 5.8.* ||^6.0 || ^7.0 || ^8.0 || ^9.0",
        "sendpulse/rest-api": "1.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flameplace\\Sendpulse\\SendpulseServiceProvider"
            ],
            "aliases": {
                "Sendpulse": "Flameplace\\Sendpulse\\Facades\\SendpulseFacade"
            }
        }
    },
    "require-dev": {
        "laravel/pint": "dev-main"
    }
}
